{
  "restApiRoot": "/api",
  "host": "0.0.0.0",
  "port": 9999,
  "remoting": {
    "context": {
      "enableHttpContext": false
    },
    "rest": {
      "normalizeHttpPath": false,
      "xml": false
    },
    "json": {
      "strict": false,
      "limit": "100kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "100kb"
    },
    "cors": {
	"origin":true,
	"credentials":true,
	"allowedHeaders": ["X-Requested-With"],
	"exposedHeaders": ["Content-Range"]
    },
    "handleErrors": false
  },
  "legacyExplorer": false
}
